{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 1,
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0
  },
  "plugins": ["prettier"]
}
